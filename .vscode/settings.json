{
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  "go.testEnvFile": "${workspaceFolder}/dev.env",
  "cSpell.words": [
    "battleye",
    "bemetrics",
    "beparser",
    "bercon",
    "buffersize",
    "buildvcs",
    "cmdline",
    "dayz",
    "debugf",
    "debugln",
    "entm",
    "envconfig",
    "errorf",
    "errorln",
    "fatalf",
    "fatalln",
    "gametype",
    "geodb",
    "geoip",
    "geolocation",
    "goarch",
    "goflags",
    "goos",
    "infof",
    "infoln",
    "keepalive",
    "ldflags",
    "loglevel",
    "logrus",
    "lzma",
    "mmdb",
    "oschwald",
    "promhttp",
    "rcon",
    "rumblefrog",
    "sethvargo",
    "sirupsen",
    "tracef",
    "traceln",
    "trimpath",
    "urfave",
    "warnf",
    "warnln",
    "woozymasta"
  ],
  "cSpell.ignorePaths": [
    "go.mod", "go.sum", ".vscode/**", ".github/**", "grafana/*.json"
  ]
}
