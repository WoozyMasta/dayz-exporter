{
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  "go.testEnvFile": "${workspaceFolder}/dev.env",
  "cSpell.words": [
    "buffersize", "loglevel", "geoip", "geodb",
    "dayz", "rcon", "bercon", "beparser", "bemetrics", "BattlEye",
    "mmdb", "sirupsen", "urfave", "logrus", "woozymasta", "oschwald",
    "rumblefrog", "sethvargo", "envconfig", "keepalive", "gametype",
    "GOOS", "GOARCH", "GOFLAGS", "GOARCH",
    "lzma", "ldflags", "buildvcs", "trimpath",
    "Tracef", "Traceln", "Debugf", "Debugln",
    "Infof", "Infoln", "Warnf", "Warnln",
    "Errorf", "Errorln", "Fatalf", "Fatalln"
  ],
  "cSpell.ignorePaths": [
    "go.mod", "go.sum", ".vscode/**", ".github/**", "grafana/*.json"
  ]
}
